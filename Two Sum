// Input: nums = [2,7,11,15], target = 9
// Output: [0,1]
// Explanation: Because nums[0] + nums[1] == 9, we return [0, 1].

void main() {
  List<int> list = [5, 8, 18, 15, 7, 8, 13];
  int target = 18;
  print(twoSum(list, target));
}

List<int> twoSum(List<int> list, int sum) {
  List<int> twoSum = [];
  int no = list.length;

  for (int i = 0; i < no; i++) {
    for (int j = i + 1; j < no; j++) {
      if (list[i] + list[j] == sum) {
        return [i, j];
      }
    }
  }
  return[];
}
